{
  "name": "@matech/monday-mcp",
  "version": "0.0.1",
  "description": "A Model Context Protocol (MCP) server to use Monday API",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mcp-first": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "mcp",
    "model-context-protocol",
    "mcp-server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/GabrielGomesBarbosa/node-mcp-first.git"
  },
  "bugs": {
    "url": "https://github.com/GabrielGomesBarbosa/node-mcp-first/issues"
  },
  "homepage": "https://github.com/GabrielGomesBarbosa/node-mcp-first#readme",
  "author": "Gabriel Barbosa <gabriel-gomes-barbosa@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "inspect": "npm run build && DANGEROUSLY_OMIT_AUTH=true npx @modelcontextprotocol/inspector node dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.21.1",
    "@mondaydotcomorg/api": "^11.0.2",
    "@types/node": "^24.10.0",
    "typescript": "^5.9.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6"
  }
}
